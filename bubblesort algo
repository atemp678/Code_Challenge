function swap(i, j) {
  return [j, i];
}

export function solution(input) {
  for (let i = 0; i < input.length; i++) {
    for (let j = 1; j < input.length; j++) {
      if (input[i] < input[j]) {
        console.log(input);
        let newPosition = swap(input[i], input[j]);
        return [...newPosition];
      }
      console.log(newPosition);
    }
  }
};